<p id="notice"><%= notice %></p>

<h1>Listing <%= AuthGroup.new.table_name_to_show %></h1>

<table>
  <thead>
    <tr>
      <th>Acciones:</th>
      <th><%= link_to Odt.new.table_name_to_show, odts_path %></th>
      <th><%= link_to Gg.new.table_name_to_show, ggs_path %></th>
      <th><%= link_to Inversion.new.table_name_to_show, inversions_path %></th>
      <th><%= link_to Employee.new.table_name_to_show, employees_path %></th>
      <th><%= link_to Retencione.new.table_name_to_show, retenciones_path %></th>
      <th><%= link_to Pool.new.table_name_to_show, pools_path %></th>
      <th><%= link_to RazonSocial.new.table_name_to_show, razon_socials_path %></th>
      <th><%= link_to AuthGroup.new.table_name_to_show, auth_groups_path %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_groups.each do |auth_group| %>
      <tr>
        <td><%= auth_group.name %>:</td>
        <td><%= auth_group.odt %></td>
        <td><%= auth_group.gg %></td>
        <td><%= auth_group.inversion %></td>
        <td><%= auth_group.employee %></td>
        <td><%= auth_group.retencione %></td>
        <td><%= auth_group.pool %></td>
        <td><%= auth_group.razon_social %></td>
        <td><%= auth_group.auth_group %></td>
        <td><%= link_to 'Show', auth_group %></td>
        <td><%= link_to 'Edit', edit_auth_group_path(auth_group) %></td>
        <td><%= link_to 'Destroy', auth_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New '+ AuthGroup.new.table_name_to_show, new_auth_group_path %>
