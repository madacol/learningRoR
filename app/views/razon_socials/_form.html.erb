<%= form_for(@razon_social, namespace: namespace_id, :html=> {:id =>  namespace_id}) do |f| %>
  <% if @razon_social.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@razon_social.errors.count, "error") %> prohibited this razon_social from being saved:</h2>
      <ul>
      <% @razon_social.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :rif_ci,'RIF o C.I.', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :rif_ci, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :name, 'Nombre', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :name, class: "form-control", :required => true %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :description, 'Descripción', class: "control-label" %><br>
      </div>
    <div class="col-sm-10">
      <%= f.text_area :description, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :percent_retencione_iva, 'Porcentaje de retencione por IVA', class: "control-label" %><br>
      </div>
    <div class="col-sm-10">
      <%= f.text_field :percent_retencione_iva, class: "form-control", value:'0' %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :phone, 'Número de Teléfono', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :phone, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :email, class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :email, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :address, 'Dirección', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_area :address, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :contact_name,'Nombre de contacto', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :contact_name, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :contact_phone,'Número de Teléfono del contacto', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :contact_phone, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :contact_email, 'Email de contacto', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :contact_email, class: "form-control" %>
    </div>
  </div>
<% end %>