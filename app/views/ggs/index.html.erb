<p id="notice"><%= notice %></p>

<h1>Listing Ggs</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ggs.each do |gg| %>
      <tr>
        <td><%= gg.code %></td>
        <td><%= gg.description %></td>
        <td><%= link_to 'Show', gg %></td>
        <td><%= link_to 'Edit', edit_gg_path(gg) %></td>
        <td><%= link_to 'Destroy', gg, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gg', new_gg_path %>
