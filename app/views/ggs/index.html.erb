<p id="notice"><%= notice %></p>

<h1>Listing <%= Gg.new.table_name_to_show %></h1>

<p><%= render 'form', gg: @gg, btn_name: 'Nuevo Registro', id: 'new_gg' %> </p>

<div class="table-responsive">
  <table  class="table table-sm table-hover table-striped table-bordered">
  <thead>
    <tr>
      <th>Code</th>
      <th>Alias</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ggs.each do |gg| %>
      <tr>
        <td><%= gg.code %></td>
        <td><%= gg.description %></td>
        <td><%= render 'form', gg: gg, btn_name: 'Editar', id: "edit_gg_#{gg.id}" %></td>
        <td><%= link_to 'Ã—', gg, method: :delete, data: { confirm: 'Are you sure?' }, class: "close equis" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
