<%= form_for(@retencione) do |f| %>
  <% if @retencione.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@retencione.errors.count, "error") %> prohibited this retencione from being saved:</h2>

      <ul>
      <% @retencione.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :code_factura %><br>
    <%= f.text_field :code_factura %>
  </div>
  <div class="field">
    <%= label_tag( 'retencione_razon_social_id', RazonSocial.new.table_name_to_show ) %><br>
    <%= f.select( :razon_social_id, options_for_select( RazonSocial.all.collect { |e| [ e.name, e.id ] }, @retencione.razon_social_id ), :prompt => '--Seleccionar--' ) %>
  </div>
  <div class="field">
    <%= f.label :tipo %><br>
    <%= f.number_field :tipo %>
  </div>
  <div class="field">
    <%= f.label :monto %><br>
    <%= f.text_field :monto %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :status %><br>
    <%= f.number_field :status %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
