<%= form_for(@retencione, namespace: namespace_id, :html=> {:id => namespace_id}) do |f| %>
  <% if @retencione.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@retencione.errors.count, "error") %> prohibited this @retencione from being saved:</h2>
      <ul>
      <% @retencione.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :tipo, "Tipo de comprobante"%><br>
    </div>
    <div class="col-sm-10">
      <%= f.select(:tipo, options_for_select( Retencione.tipos.keys.to_a), { prompt: '--Seleccionar--'}, { class: "form-control c-select chosen-select" }) %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :code_factura, 'Código', class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :code_factura, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
    <%= f.label :razon_social_id, RazonSocial.new.table_name_to_show %>
    </div>
    <div class="col-sm-10">
    <%= f.select( :razon_social_id, options_for_select( RazonSocial.all.collect{ |e| [e.record_name_to_show, e.id ] }, @retencione.razon_social_id ), { prompt: '--Seleccionar--'}, { class: "form-control c-select chosen-select" }) %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :monto, class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_field :monto, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :description, "Descripción", class: "control-label" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.text_area :description, class: "form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-2">
      <%= f.label :status, "Estado" %><br>
    </div>
    <div class="col-sm-10">
      <%= f.select(:status, options_for_select( Retencione.statuses.keys.to_a,@retencione.status), { prompt: '--Seleccionar--'}, { class: "form-control c-select chosen-select" }) %>
    </div>
  </div>
<% end %>