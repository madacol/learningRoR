<p id="notice"><%= notice %></p>

<h1>Listing <%= Retencione.new.table_name_to_show %></h1>

<table>
  <thead>
    <tr>
      <th>Code factura</th>
      <th><%= RazonSocial.new.table_name_to_show %></th>
      <th>Tipo</th>
      <th>Monto</th>
      <th>Description</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @retenciones.each do |retencione| %>
      <tr>
        <td><%= retencione.code_factura %></td>
        <td><%= link_to retencione.razon_social.record_name_to_show, retencione.razon_social %></td>
        <td><%= retencione.tipo %></td>
        <td><%= retencione.monto %></td>
        <td><%= retencione.description %></td>
        <td><%= retencione.status %></td>
        <td><%= link_to 'Show', retencione %></td>
        <td><%= link_to 'Edit', edit_retencione_path(retencione) %></td>
        <td><%= link_to 'Destroy', retencione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New '+ Retencione.new.table_name_to_show, new_retencione_path %>
